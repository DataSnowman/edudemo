{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "edudata"
		},
		"edudemostorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'edudemostorage'"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/edudemostorage')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "edudemostorage",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"fileName": "Student Profile.csv",
						"folderPath": "input",
						"container": "data"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": [
					{
						"name": "CRMID",
						"type": "String"
					},
					{
						"name": "LAST_NAME",
						"type": "String"
					},
					{
						"name": "FIRST_NAME",
						"type": "String"
					},
					{
						"name": "MIDDLE_NAME",
						"type": "String"
					},
					{
						"name": "ADMIT_TERM",
						"type": "String"
					},
					{
						"name": "ADMIT_DESCR",
						"type": "String"
					},
					{
						"name": "ACAD_CAREER",
						"type": "String"
					},
					{
						"name": "ADMIT_TYPE",
						"type": "String"
					},
					{
						"name": "ADMIT_TYPE_DESCR",
						"type": "String"
					},
					{
						"name": "APPL_SOURCE",
						"type": "String"
					},
					{
						"name": "ACAD_PROG",
						"type": "String"
					},
					{
						"name": "ACAD_PROG_DESCR",
						"type": "String"
					},
					{
						"name": "ACAD_PLAN",
						"type": "String"
					},
					{
						"name": "ACAD_PLAN_DESCR",
						"type": "String"
					},
					{
						"name": "CURRENT_PROGRAM",
						"type": "String"
					},
					{
						"name": "CURRENT_PROGRAM_DESCR",
						"type": "String"
					},
					{
						"name": "CURRENT_PLAN",
						"type": "String"
					},
					{
						"name": "CURRENT_PLAN_DESCR",
						"type": "String"
					},
					{
						"name": "GENDER",
						"type": "String"
					},
					{
						"name": "AGE_BY_YEARS",
						"type": "String"
					},
					{
						"name": "ADDRESS1",
						"type": "String"
					},
					{
						"name": "ADDRESS2",
						"type": "String"
					},
					{
						"name": "CITY",
						"type": "String"
					},
					{
						"name": "STATE",
						"type": "String"
					},
					{
						"name": "POSTAL",
						"type": "String"
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/edudemostorage')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/edudemostorageSink')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "edudemostorage",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"fileName": "student.csv",
						"folderPath": "output",
						"container": "data"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/edudemostorage')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/edudemostorage')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('edudemostorage_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/studentProfile')]",
			"type": "Microsoft.DataFactory/factories/dataflows",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "WranglingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"name": "edudemostorage",
							"script": "",
							"dataset": {
								"referenceName": "edudemostorage",
								"type": "DatasetReference"
							}
						}
					],
					"sinks": [
						{
							"name": "Sink1",
							"dataset": {
								"referenceName": "edudemostorageSink",
								"type": "DatasetReference"
							},
							"script": "sink(allowSchemaDrift: true,\n\tvalidateSchema: false,\n\tskipDuplicateMapInputs: true,\n\tskipDuplicateMapOutputs: true) ~> Sink1"
						}
					],
					"script": "section Section1;\r\nshared edudemostorage = let\r\n  AdfDoc = AzureStorage.BlobContents(\"https://edudemostorage.blob.core.windows.net/data/input/Student Profile.csv\"),\r\n  Csv = Csv.Document(AdfDoc, [Delimiter = \",\", Encoding = TextEncoding.Utf8, QuoteStyle = QuoteStyle.Csv]),\r\n  PromotedHeaders = Table.PromoteHeaders(Csv, [PromoteAllScalars = true])\r\nin\r\n  PromotedHeaders;\r\nshared UserQuery = let\r\n  Source = edudemostorage,\r\n  #\"Removed columns\" = Table.RemoveColumns(Source, {\"ADMIT_DESCR\", \"ADMIT_TYPE_DESCR\", \"ACAD_PROG_DESCR\", \"ACAD_PLAN_DESCR\", \"CURRENT_PROGRAM_DESCR\", \"CURRENT_PLAN_DESCR\"}),\r\n  #\"Changed column type\" = Table.TransformColumnTypes(#\"Removed columns\", {{\"AGE_BY_YEARS\", Int64.Type}, {\"ADMIT_TERM\", Int64.Type}}),\r\n  #\"Removed columns 1\" = Table.RemoveColumns(#\"Changed column type\", {\"ADMIT_TYPE\", \"APPL_SOURCE\", \"ACAD_PROG\", \"ACAD_PLAN\", \"CURRENT_PROGRAM\", \"CURRENT_PLAN\"})\r\nin\r\n  #\"Removed columns 1\";\r\n"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/edudemostorage')]",
				"[concat(variables('factoryId'), '/datasets/edudemostorageSink')]"
			]
		}
	]
}